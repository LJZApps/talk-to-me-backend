<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">
    <route id="api_v1_create_user" path="/user" methods="PUT" controller="App\Controller\API\ApiUserController::createUser" />
    <route id="api_v1_get_user" path="/user/{uid}" methods="GET" controller="App\Controller\API\ApiUserController::getUserData" />
</routes>